  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "generate-api": "cd api && npx @openapitools/openapi-generator-cli generate -i stocks.yaml -g go-gin-server -o server --git-repo-id StockPop/api/server --git-user-id SC4RECOIN --additional-properties=featureCORS=true,serverPort=5500",
