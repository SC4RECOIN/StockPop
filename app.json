{
  "expo": {
    "name": "StockPop",
    "slug": "stock-pop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pop-far.png",
    "scheme": "stockpop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/pop-far.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pop-far.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sc4recoin.stockpop"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "434ac5e0-a09f-4e8a-8e43-ad04d4ebfcc6"
      }
    },
    "owner": "sc4recoin"
  }
}
